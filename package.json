{
  "version": "0.1.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "use-save-files-as-zip",
  "author": "Jesus Jimenez Cordero",
  "module": "dist/example.esm.js",
  "size-limit": [
    {
      "path": "dist/example.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/example.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/shortid": "0.0.29",
    "husky": "^5.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "jszip": "^3.6.0",
    "jszip-utils": "^0.1.0",
    "save-as": "^0.1.8",
    "shortid": "^2.2.16"
  }
}
